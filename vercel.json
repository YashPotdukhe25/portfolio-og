{
  "version": 2,
  "builds": [
    {
      "src": "server/index-prod.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/**",
          "server/**",
          "shared/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index-prod.ts"
    },
    {
      "src": "/(.*)",
      "dest": "server/index-prod.ts"
    }
  ]
}